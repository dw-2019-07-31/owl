<p id="notice"><%= notice %></p>

<h1>New Products</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @new_products.each do |new_product| %>
      <tr>
        <td><%= link_to 'Show', new_product %></td>
        <td><%= link_to 'Edit', edit_new_product_path(new_product) %></td>
        <td><%= link_to 'Destroy', new_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New New Product', new_new_product_path %>
